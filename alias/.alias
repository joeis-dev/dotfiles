alias nv=$EDITOR
alias st="git status"
alias zcfg="$EDITOR $HOME/.zshrc"
alias vicfg="$EDITOR $HOME/.config/nvim/init.vim"
alias mine="java -jar $CUSTOM_PACKAGES/minecraft/TLauncher-2.8.jar"
alias deploy-env="sh $HOME/.dotfiles/deploy.sh"
alias undeploy-env="sh $HOME/.dotfiles/undeploy.sh"
alias ls="lsd -la"
alias top="bpytop"
alias yoump3="youtube-dl -o '%(title)s.%(ext)s' --no-warnings -i -w -x --embed-thumbnail --audio-format mp3"
alias ydl="youtube-dl -o '%(title)s.%(ext)s' --no-warnings -i -w"
alias reset-kde="kquitapp5 plasmashell && kstart5 plasmashell"
alias logout="pkill -u $USER"
alias rdir="rm -fRv"
alias rm="rm -fv"
alias updategrub="sudo grub2-mkconfig -o /boot/efi/EFI/fedora/grub.cfg"
alias docker="podman"
alias pps="podman ps --format=$PODMAN_FORMAT"
alias conda-update="conda update -n base -c defaults conda"
alias glog="git log --graph --oneline"
